<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Bookinghotel">
        <table name="bookinghotel" schema="java-final" catalog=""/>
        <attributes>
            <id name="idbooking">
                <column name="idbooking"/>
            </id>
            <basic name="iduser">
                <column name="iduser"/>
            </basic>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="idhotel">
                <column name="idhotel"/>
            </basic>
            <basic name="idroom">
                <column name="idroom"/>
            </basic>
            <basic name="checkin">
                <column name="checkin"/>
            </basic>
            <basic name="checkout">
                <column name="checkout"/>
            </basic>
            <basic name="adult">
                <column name="adult" nullable="false" precision="50"/>
            </basic>
            <basic name="child">
                <column name="child" nullable="false" precision="50"/>
            </basic>
            <basic name="sophong">
                <column name="sophong" precision="10"/>
            </basic>
            <basic name="totalprice">
                <column name="totalprice"/>
            </basic>
            <basic name="statusbooking">
                <column name="statusbooking" nullable="false"/>
            </basic>
            <basic name="datebooking">
                <column name="datebooking" nullable="false" precision="-1"/>
            </basic>
            <basic name="representative">
                <column name="representative"/>
            </basic>
            <basic name="phonecontact">
                <column name="phonecontact"/>
            </basic>
            <basic name="emailcontact">
                <column name="emailcontact"/>
            </basic>
            <basic name="review">
                <column name="review" nullable="false" precision="10"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Voucher">
        <table name="voucher" schema="java-final" catalog=""/>
        <attributes>
            <id name="idvoucher">
                <column name="idvoucher"/>
            </id>
            <basic name="discount">
                <column name="discount"/>
            </basic>
            <basic name="soluong">
                <column name="soluong" precision="255"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
