<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Service">
        <table name="service" schema="java-final" catalog=""/>
        <attributes>
            <id name="stt">
                <column name="stt" precision="11"/>
            </id>
            <basic name="nameService">
                <column name="name_service" nullable="false"/>
            </basic>
            <basic name="price">
                <column name="price"/>
            </basic>
            <basic name="amount">
                <column name="amount" precision="255"/>
            </basic>
            <basic name="imgService">
                <column name="img_service" length="1000"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Messagebycustomer">
        <table name="messagebycustomer" schema="java-final" catalog=""/>
        <attributes>
            <id name="stt">
                <column name="stt" precision="255"/>
            </id>
            <basic name="iduser">
                <column name="iduser"/>
            </basic>
            <basic name="message">
                <column name="message" length="1000"/>
            </basic>
            <basic name="rep">
                <column name="rep" nullable="false" precision="255"/>
            </basic>
            <basic name="datesend">
                <column name="datesend" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Repmessage">
        <table name="repmessage" schema="java-final" catalog=""/>
        <attributes>
            <id name="stt">
                <column name="stt" precision="255"/>
            </id>
            <basic name="sttmessagesend">
                <column name="sttmessagesend" precision="255"/>
            </basic>
            <basic name="repcontent">
                <column name="repcontent" nullable="false" length="1000"/>
            </basic>
            <basic name="daterep">
                <column name="daterep" nullable="false" precision="-1"/>
            </basic>
            <basic name="userread">
                <column name="userread" nullable="false" precision="255"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Product">
        <table name="product" schema="java-final" catalog=""/>
        <attributes>
            <id name="sttproduct">
                <column name="sttproduct" precision="255"/>
            </id>
            <basic name="nameproduct">
                <column name="nameproduct"/>
            </basic>
            <basic name="priceduct">
                <column name="priceduct"/>
            </basic>
            <basic name="imgproduct">
                <column name="imgproduct" length="1000"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Product">
        <table name="product" schema="java-final" catalog=""/>
        <attributes>
            <id name="sttproduct">
                <column name="sttproduct"/>
            </id>
            <basic name="nameproduct">
                <column name="nameproduct"/>
            </basic>
            <basic name="priceduct">
                <column name="priceduct"/>
            </basic>
            <basic name="imgproduct">
                <column name="imgproduct" length="1000"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
