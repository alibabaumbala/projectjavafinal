<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Voucher">
        <table name="voucher" schema="java-final" catalog=""/>
        <attributes>
            <id name="idvoucher">
                <column name="idvoucher"/>
            </id>
            <basic name="discount">
                <column name="discount"/>
            </basic>
            <basic name="soluong">
                <column name="soluong" precision="255"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
